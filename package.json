{
    "name": "sdk",
    "version": "0.0.0",
    "private": false,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "build:sdk": "turbo run build --filter=./packages/sdk",
        "test:sdk": "turbo run test --filter=./packages/sdk"
    },
    "devDependencies": {
        "prettier": "latest",
        "turbo": "latest"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "dependencies": {},
    "packageManager": "pnpm@7.12.2"
}
